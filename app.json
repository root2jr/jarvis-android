{
  "expo": {
    "name": "JARVIS",
    "slug": "JARVIS",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/jarvis-icon.png",
    "scheme": "jarvis",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/jarvis-icon.png",
        "backgroundColor": "skyblue",
        "borderRadius": 40
      },
      "edgeToEdgeEnabled": true,
      "package": "com.jayaram1810.JARVIS"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/jarvis-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "skyblue",
          "borderRadius": 40
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "5a9db27b-87ee-4bfa-bd8f-ea806380a2f0"
      }
    }
  }
}
